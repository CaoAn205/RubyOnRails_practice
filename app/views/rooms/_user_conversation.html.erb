<% if users.present? %>
  <% users.each do |user| %>
    <%= link_to user_room_room_messages_path(user.id, room.id), method: :get, class: "no-text-decoration normal-color" do %>
      <div class="search-result-item text-sm-left"><%= user.name %></div>
    <% end %>
  <% end %>
<% else %>
  <div class="p-3">
    No results found
  </div>
<% end %>
